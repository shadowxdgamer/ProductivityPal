<Application x:Class="ProductivityPal.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProductivityPal"
             xmlns:converters="clr-namespace:ProductivityPal.Converters"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <converters:PriorityToColorConverter x:Key="PriorityToColorConverter"/>
        <converters:StringNotEmptyToVisibilityConverter x:Key="StringNotEmptyToVisibilityConverter"/>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    </Application.Resources>
</Application>
